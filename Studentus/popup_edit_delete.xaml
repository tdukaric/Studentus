<UserControl x:Class="Studentus.popup_edit_delete"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot">
        <StackPanel>
            <TextBlock Text="Edit or delete" TextAlignment="Center" FontSize="30" Margin="10,0" />
            <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" Margin="0,10">
                <Button x:Name="btnEdit" Content="edit" Width="215" />
                <Button x:Name="BtnDelete" Content="delete"   Width="215" />
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
